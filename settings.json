{
  "editor.fontSize": 20,
  "editor.rulers": [120],
  "editor.suggestSelection": "first",
  "editor.renderWhitespace": "all",
  "debug.toolBarLocation": "docked",
  "python.languageServer": "Pylance",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "go.formatTool": "gofmt",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.snippetSuggestions": "none",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "gopls": {
    "usePlaceholders": true, // add parameter placeholders when completing a function
    "wantCompletionDocumentation": true // for documentation in completion items
  },
  "editor.formatOnSaveMode": "file", // required to format on save
  "vs-code-prettier-eslint.prettierLast": false, // set as "true" to run 'prettier' last not first
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Vue.volar"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[lua]": {
    "editor.formatOnSave": true
  },
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "html",
    "vue",
    "jsx"
  ],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 10000,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.wordWrapColumn": 200,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.format.enable": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "workbench.iconTheme": "vscode-icons",
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[dockerfile]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[conf]": {
    "editor.formatOnSave": false
  },
  "files.associations": {
    "*.ejs": "html",
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript",
    ".jsx": "javascriptreact"
  },
  "emmet.includeLanguages": {
    "ejs": "html"
  },
  "minapp-vscode.disableAutoConfig": true,
  "excalidraw.image": {
    "exportScale": 1,
    "exportWithBackground": true,
    "exportWithDarkMode": false
  },
  "hediet.vscode-drawio.resizeImages": null,
  "window.commandCenter": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.formatOnPaste": true,
  "go.toolsManagement.autoUpdate": true,
  "protoc": {
    "path": "/Users/fengjianxin/opt/protoc/current/bin/protoc",
    "options": ["--proto_path=protos/v3", "--proto_path=protos/v2"]
  },
  "clang-format.style": "google",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Default High Contrast",
  "markdown-preview-enhanced.codeBlockTheme": "vue.css",
  "markdown-preview-enhanced.previewTheme": "vue.css",
  "markdown-preview-enhanced.enablePreviewZenMode": false,
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  }
}
